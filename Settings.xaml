<Window x:Class="Connect4.Settings"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Connect4"
        mc:Ignorable="d"
        Title="Settings" Height="800" Width="500">
    <Grid>
        <TextBlock FontSize="30" Margin="72,58,291.6,672.4">StartTime</TextBlock>
        <ComboBox Name="StartTimeText" IsEditable="True" Margin="328,58,95.6,672.4"></ComboBox>

        <TextBlock FontSize="30" Margin="72,129,291.6,601.4">AddTime</TextBlock>
        <ComboBox Name="AddTimeText" IsEditable="True" Margin="328,129,95.6,601.4"></ComboBox>

        <Button FontSize="50" Name="StartGame" Click="ReturnToManu" Canvas.Left="142" Canvas.Top="84" Margin="171,643,164.6,48.4">Start</Button>
       
    </Grid>
</Window>
